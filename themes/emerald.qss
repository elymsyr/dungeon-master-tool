/* Emerald Theme */
QMainWindow, QWidget {
    background-color: #0d1b1e;
    color: #e0f2f1;
}

QTabWidget::pane {
    border: 1px solid #1a3d3c;
    background: #122b2a;
}

QTabBar::tab:selected {
    background: #00897b;
    color: white;
}

QPushButton#primaryBtn {
    background-color: #00897b;
}

QGroupBox {
    border: 2px solid #1a3d3c;
    color: #4db6ac;
}
